datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Client {
  id                Int      @id @default(autoincrement())
  createdAt         DateTime @default(now())
  messageReceivedAt DateTime @default(now())
  ipAddress         String
  port              String

  @@unique([ipAddress, port], name: "ipAddressWithPort")
}
